  "bugs": {
    "url": "https://github.com/chrisl8/Witchazzan/issues"
  },
  "homepage": "https://witchazzan.space/",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@parcel/packager-raw-url": "^2.6.0",
    "@parcel/packager-xml": "^2.7.0",
    "@parcel/reporter-bundle-analyzer": "^2.7.0",
    "@parcel/transformer-webmanifest": "^2.5.0",
    "@parcel/transformer-xml": "^2.7.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "parcel": "^2.5.0",
    "path-browserify": "^1.0.1",
    "pm2": "^5.2.0",
    "prettier": "^2.0.2",
    "process": "^0.11.10"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
